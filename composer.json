{
    "name": "paradisesecurity/service-registry",
    "type": "library",
    "description": "Register services in PHP applications.",
    "homepage": "https://paradisesecurity.work",
    "license": "MIT",
    "authors": [
        {
            "name": "Paradise Security",
            "email": "joe@paradisesecurity.work",
            "homepage": "https://paradisesecurity.work",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ParadiseSecurity\\Component\\ServiceRegistry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "ParadiseSecurity\\Component\\ServiceRegistry\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
